Get-Service | Where-Object {$_.status -eq "running"} | Where-Object {$_.DisplayName -like "*NGN*"} | ForEach{
$service = get-service $_.Name
$service.Stop()
}

Get-Service | Where-Object {$_.status -eq "running"} | Where-Object {$_.DisplayName -like "*SGT3*"} | ForEach{
$service = get-service $_.Name
$service.Stop()
}